pre-desktop:
  locale-system: en_US
  locales-present:
    - nl_NL
    - fr_FR
  extra-packages:
    - vim
    - git
  # https://en.wikipedia.org/wiki/Ubuntu-restricted-extras
  install-restricted-extras: true
  # https://www.microsoft.com/typography/fontpack/eula.htm
  accept-mscorefonts-eula: true
post-desktop:
  user: vagrant
  autologin: true
