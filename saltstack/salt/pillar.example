user: vagrant
pre-desktop:
  locale-system: nl_NL
  locales-present:
    - fr_FR
  timezone: Europe/Amsterdam
  extra-packages:
    - vim
    - git
  install-restricted-extras: true
  accept-mscorefonts-eula: true
post-desktop:
  autologin: true
customize-desktop:
  launcher-favorites-applications:
    - application://firefox.desktop
